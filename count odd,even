#include<stdio.h>
int main()
{
	int A[10],i,n,odd_c,even_c;
	printf("enter n");
	scanf("%d",&n);
	printf("enter the array element:");
	for(i=0;i<n;i++)
	scanf("%d",&A[i]);
    for(i=0;i<n;i++)
	{
		if (A[i]%2==1)
		odd_c++;
		else
		even_c++;
	}
	printf("\n the odd count=%d",odd_c);
	printf("\n the even count=%d",even_c);
	return 0;
}
